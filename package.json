{
	"name": "resilio-sync",
	"version": "2.0.1",
	"description": "Resilio Sync Abstraction",
	"license": "MIT",
	"repository": "EdJoPaTo/resilio-sync-nodejs-abstraction",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "https://edjopato.de"
	},
	"type": "commonjs",
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc && xo"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		"resilio",
		"sync"
	],
	"dependencies": {
		"write-json-file": "^4.1.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^1.0.1",
		"@types/node": "^14.14.10",
		"del-cli": "^3.0.0",
		"typescript": "^4.2.3",
		"xo": "^0.38.1"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "error"
		},
		"overrides": [
			{
				"files": [
					"source/config.ts"
				],
				"rules": {
					"@typescript-eslint/camelcase": "off",
					"camelcase": "off"
				}
			}
		]
	}
}
